{
    "name": "browserbox",
    "description": "browserbox test app",
    "version": "0.0.1",
    "manifest_version": 2,
    "offline_enabled": false,
    "permissions": [
        "http://*/*", 
        "https://*/*", 
        {
            "socket": [
                "tcp-connect"
            ]
        }
    ],
    "app": {
        "background": {
            "scripts": ["example/background.js"]
        }
    },
    "icons": {
        "128": "example/icon-128.png"
    }
}